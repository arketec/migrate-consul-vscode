{
  "name": "migrate-consul-vscode",
  "displayName": "migrate-consul-vscode",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "migrate-consul-vscode-sidebar-view",
          "title": "Migrate Consul",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "migrate-consul-vscode-sidebar-view": [
        {
          "type": "webview",
          "id": "migrate-consul-vscode-sidebar",
          "name": "Create Migration",
          "icon": "assets/icon.svg",
          "contextualTitle": "Create Migration"
        }
      ]
    },
    "commands": [
      {
        "command": "migrate-consul-vscode.install",
        "title": "Migrate Consul: Install"
      },
      {
        "command": "migrate-consul-vscode.new-project",
        "title": "Migrate Consul: New Project",
        "when": "workbenchState == 'workspace'"
      },
      {
        "command": "migrate-consul-vscode.new-migration",
        "title": "Migrate Consul: New Migration",
        "when": "workbenchState == 'workspace'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./dist/test/runTest.js",
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish",
    "webpack": "rm -rf ./dist/* && webpack --mode development",
    "package": "rm -rf *.vsix && yarn webpack && vsce package",
    "publish": "yarn package && yarn vscode:publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react-dom": "^18.2.7",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ejs": "^3.1.9",
    "fs-jetpack": "^5.1.0",
    "migrate-consul": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
